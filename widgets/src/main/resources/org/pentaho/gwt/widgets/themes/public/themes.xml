    <file>bootstrap/css/bootstrap-namespaced.css</file>
  </ruby>
  <onyx display-name="Onyx" system="true">
    <file>globalOnyx.css</file>
    <file>bootstrap/css/bootstrap-namespaced.css</file>
  </onyx>
  <crystal display-name="Crystal" system="true">
    <file>../base.css</file>
